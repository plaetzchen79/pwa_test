{
  "index": "/index.html",
  "push": {
    "showNotifications": true,
    "backgroundOnly": false
  },
  "assetGroups": [
    {
      "name": "firstpwa",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"]
      }
    }
  ]
}
